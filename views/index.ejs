<!--include Header-->
    <%- include('include/_header')%>
<!--/include Header-->

<!--Main Site-->
<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex justify-between">
            <a href="/add-user" class="border-shadow">
                <span class="text-gradient">New User <i class="fas fa-user"></i></span>
            </a>
            <a href="/search" class="border-shadow">
                <span class="text-gradient">Search <i class="fa-solid fa-magnifying-glass"></i></span>
            </a>
            <a href="/reports" class="border-shadow">
                <span class="text-gradient">Reports <i class="fa-solid fa-chart-simple"></i></span>
            </a>
        </div>
        <!--form handling-->
        <form action="/" method="post">
            <table class="table">
                <caption class="table-caption">Current Users Information</caption>
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>UserName</th>
                        <th>ePwd</th>
                        <th>UserGroup</th>
                        <th>AUT</th>
                        <th>AppEnv</th>
                        <th>Info</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody class="tbody">
                    <!--include Footer-->
                        <%- include('include/_show')%>
                    <!--/include Footer-->
                </tbody>
            </table>
        </form>
    </div>
</main>
<!--/Main Site-->

<!--include Footer-->
<%- include('include/_footer')%>
<!--/include Footer-->